<form method="post" action="{{ path('app_nftcollection_delete', {'id': nftcollection.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ nftcollection.id) }}">
    <button class="btn">Delete</button>
</form>
